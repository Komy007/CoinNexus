{
  "name": "coinnexus",
  "version": "1.0.0",
  "description": "암호화폐 선물 거래에 특화된 정보 및 커뮤니티 플랫폼",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "build-all": "node build.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm run dev",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "cd server && npm install && cd ../client && npm install",
    "postinstall": "node build.js",
    "heroku-postbuild": "node build.js"
  },
  "keywords": [
    "cryptocurrency",
    "trading",
    "binance",
    "react",
    "nodejs",
    "sqlite"
  ],
  "author": "Komy007",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "sequelize": "^6.35.2",
    "sqlite3": "^5.1.6",
    "axios": "^1.6.2",
    "socket.io": "^4.7.4",
    "dotenv": "^16.3.1",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "compression": "^1.7.4",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}